# Package: protoc_plugin
# Version: 20.0.1
# Executable: protoc-gen-dart
# Script: protoc_plugin
if [ -f /home/karim/snap/flutter/common/flutter/.pub-cache/global_packages/protoc_plugin/bin/protoc_plugin.dart-2.18.0.snapshot ]; then
  dart "/home/karim/snap/flutter/common/flutter/.pub-cache/global_packages/protoc_plugin/bin/protoc_plugin.dart-2.18.0.snapshot" "$@"
  # The VM exits with code 253 if the snapshot version is out-of-date.
  # If it is, we need to delete it and run "pub global" manually.
